{
   "browser_action": {
      "default_icon": {
         "16": "assets/icons/16.png",
         "32": "assets/icons/32.png",
         "48": "assets/icons/48.png",
         "128": "assets/icons/128.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {  
      "all_frames": true,
      "js": [ "betterbrime.js" ],
      "matches": [ "*://*.brimelive.com/*" ]
   } ],
   "description": "BetterBrime enhances Brime with new features, emotes, and more.",
   "homepage_url": "https://betterbri.me/",
   "icons": {
      "48": "assets/icons/48.png",
      "128": "assets/icons/128.png"
   },
   "manifest_version": 2,
   "name": "BetterBrime",
   "permissions": [ "*://*.brimelive.com/*", "*://*.betterbri.me/*"],
   "short_name": "BB",
   "version": "1.18",
   "web_accessible_resources": ["betterbrime.js", "brimelive.css", "assets/icons/*", "assets/fonts/*", "assets/Jquery/*" ]
}
