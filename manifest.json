{
   "browser_action": {
      "default_icon": {
         "16": "assets/icons/16.png",
         "32": "assets/icons/32.png",
         "48": "assets/icons/48.png",
         "128": "assets/icons/128.png"
      },
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "script.js" ],
      "matches": [ "*://*.brimelive.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "BetterBrime enhances Brime with new features, emotes, and more.",
   "icons": {
      "48": "assets/icons/48.png",
      "128": "assets/icons/128.png"
   },
   "manifest_version": 2,
   "name": "BetterBrime",
   "permissions": [ "*://*.brimelive.com/*" ],
   "version": "1.07.1",
   "web_accessible_resources": ["brimelive.css", "assets/icons/128.png", "assets/icons/32.png" ]
}
